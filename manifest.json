{
	"manifest_version": 2,

	"name": "Unrecommender",
	"description": "An extension that filters YouTube recommendations based on your subscriptions and the channel you're watching.",
	"version": "2.0.2",

	"browser_action": {
		"default_icon": {
			"16": "img/icon_16.png",
			"32": "img/icon_32.png"
		},
		"default_title": "Unrecommender"
	},
	"permissions": [
		"*://*.youtube.com/*"
	],
	"content_scripts": [
		{
			"matches": ["*://*.youtube.com/*"],
			"run_at": "document_start",
			"js": ["js/jquery-3.2.1.min.js", "js/main.js"]
		}
	],
	"icons": {
		"16": "img/icon_16.png",
		"32": "img/icon_32.png",
		"64": "img/icon_64.png",
		"128": "img/icon_128.png"
	}
}